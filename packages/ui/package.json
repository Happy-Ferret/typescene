{
  "name": "@typescene/ui",
  "version": "0.9.0",
  "description": "Strongly typed GUI toolkit for Web applications - UI module",
  "author": "Jelmer Cormont",
  "repository": "https://github.com/typescene/typescene/tree/master/packages/ui",
  "homepage": "http://typescene.org/",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist-module/index.js",
  "typings": "./typings/index.d.ts",
  "scripts": {
    "clean": "rimraf dist dist-module typings",
    "build": "npm run build-src && npm run build-src-module",
    "build-src": "tsc -p src",
    "build-src-module": "tsc -p src --module es6 --outDir dist-module",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@typescene/async": "^0.9.0"
  },
  "devDependencies": {
    "rimraf": "^2.5.4"
  }
}
