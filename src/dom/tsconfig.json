{
    "compilerOptions": {
        "outDir": "../../dist/dom/lib",
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "pretty": true,
        "declaration": true,
        "declarationDir": "../../dist/dom/typings",
        "stripInternal": true,
        "experimentalDecorators": true,
        "strict": true,
        "baseUrl": ".",
        "lib": [ "es5", "dom" ],
        "paths": {
            "@typescene/core": [
                "../../dist/core/internal/typings"
            ],
            "@typescene/core/*": [
                "../../dist/core/internal/typings/*"
            ]
        }
    },
    "include": [
        "./**.ts"
    ]
}
